<h1>Docs</h1>
  <% @docs.each do |doc| %>
    <h2><%= link_to doc.title, doc %></h2>
    <p><%= time_ago_in_words(doc.created_at) %></p>
    <p><%= truncate(doc.content, length:70) %></p>
    <p><%= link_to "Edit", edit_doc_path(doc) %>&nbsp;<%= link_to "Delete", doc, method: :delete, data: {confirm: 'Are you sure ?'} %></p>
  <% end %>
<br />
<%= link_to  "New doc", new_doc_path %>